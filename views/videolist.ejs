<!DOCTYPE HTML>
<html>
	<head>
		<title>화상 회의 목록</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="/assets/css/main.css" />
		<noscript><link rel="stylesheet" href="assets/css/noscript.css" /></noscript>
		<script type="text/javascript" src="http://code.jquery.com/jquery-3.2.0.min.js" ></script>
	</head>
	<body class="is-preload">

		<!-- Header -->
			<header id="header">
				<a href="/" class="title">Hyperspace</a>
				<nav>
					<ul>
						<li><a href="/">Home</a></li>
						<li><a href="/video" class="active">화상 회의 목록</a></li>
						<li><a href="/video/make">방만들기</a></li>
					</ul>
				</nav>
			</header>
		<!-- Wrapper -->
			<div id="wrapper">
				<!-- Main -->
					<section id="main" class="wrapper">
						<div class="inner">
							<h1 class="major">화상 회의 목록</h1>
						<%
							for(var i=0; i<list.length; i++)
							{
								var info = list[i];
						%>
							<div id="meeting<%=info.idx%>" style="border:3px solid;padding:10px;">
								<span class="image fit">
									<img src="images/<%=info.img%>" width="370" height="100"alt="" />
								</span>
								<div style="float:left;"><h3><%=info.name%></h3></div>
							<% if(info.isLock==1)
								{
							%>
								<a href="/rtc/<%=info.idx%>">
									<div style="float:right;"><img src="images/lock.png" width="30" height="30"/></div>
								</a>
							<%
								}
								else{
							%>	
								<a href="/rtc/<%=info.idx%>">
									<div style="float:right;"><img src="images/enter.png" width="30" height="30"/></div>
								</a>
							<%
								}
							%>
								<br>
							</div>
							<br>
						<%
							}
						%>
						</div>
					</section>

			</div>
			
		<!-- Footer -->
			<footer id="footer" class="wrapper alt">
				<div class="inner">
					<ul class="menu">
						<li>&copy; Untitled. All rights reserved.</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
					</ul>
				</div>
			</footer>

		<!-- Scripts -->
			<script src="assets/js/jquery.min.js"></script>
			<script src="assets/js/jquery.scrollex.min.js"></script>
			<script src="assets/js/jquery.scrolly.min.js"></script>
			<script src="assets/js/browser.min.js"></script>
			<script src="assets/js/breakpoints.min.js"></script>
			<script src="assets/js/util.js"></script>
			<script src="assets/js/main.js"></script>
	</body>
</html>
